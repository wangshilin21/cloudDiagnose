page {  
  display: block;  
  min-height: 100%;  
  background-color:white;  
}  
.title {
  width:100%;
  height:200rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size: 60rpx;
  font-weight: 700;
  color:#3963bc;
  font-family: Arial, Helvetica, sans-serif;
  margin-top: 5rpx;
  background-color: #EBEBEB;
}
.coding{
  display: flex;
  margin-top: 20rpx;
  align-items:center;
  justify-content:center;
}
.codingImage{
  border-radius:3px;
 box-shadow:rgba(0,0,0,0.15) 0 0 8px; 
 border:1px solid red;
}
.returnButton{
  margin-top:45rpx;
  display: flex;
  align-items:center;
  justify-content:center;
}